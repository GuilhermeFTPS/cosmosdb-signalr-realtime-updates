{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
        "name": "flight",
        "type": "cosmosDB",
        "databaseName": "demo",
        "collectionName": "flights",
        "createIfNotExists": true,
        "connectionStringSetting": "AzureWebJobsCosmosDBConnectionString",
        "direction": "out"
    }
  ]
}
